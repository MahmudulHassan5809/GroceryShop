<% layout('layout') %>

<div class="container mt-5">
	<div class="row">
    <div class="row wow fadeIn mt-3" id="search_result">
    </div>
		<div class="col-md-10 mx-auto">
			<table id="dtBasicExample" class="table table-striped table-bordered table-sm" cellspacing="0" width="100%">
                <thead>
                    <tr>
                        <th class="th-sm">Invoice No
                        </th>
                        <th class="th-sm">Prtoduct
                        </th>
                        <th class="th-sm">Qty
                        </th>
                        <th class="th-sm">Price
                        </th>
                        <th class="th-sm">Paid
                        </th>
                        <th class="th-sm">Delivered
                        </th>
                    </tr>
                </thead>
                <tbody>
                <% for (var i = 0; i < history.length; i++) { -%>
				<% for (var j = 0; j < _.size(history[i].order); j++) { -%>
				<tr>
                      <td><%= history[i].invoice %></td>

          					  <td><%= history[i].order[j].title %></td>
          					  <td><%= history[i].order[j].qty %></td>
          					  <td><%= history[i].order[j].price *  history[i].order[j].qty %></td>
          					  <% if (!history[i].ispaid) { -%>
          					  <td>
          						Not Paid
          					  </td>
          					  <% } else { -%>
                                <td>
          						Paid
                                </td>
          					  <% } -%>
                      <% if (!history[i].isdeliver) { -%>
                      <td>
                      	No
                      </td>
                      <% } else { -%>
                        <td>
                      	Yes
                       </td>
                      <% } -%>
          					</tr>
                  	<% } -%>
                  <% } -%>

                </tbody>
            </table>
		</div>
	</div>
</div>
